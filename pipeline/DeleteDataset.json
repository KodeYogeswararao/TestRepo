{
	"name": "DeleteDataset",
	"properties": {
		"activities": [
			{
				"name": "DeleteFile",
				"type": "Delete",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "FileDelete",
						"value": "Delete file in input container and logged it into output container"
					}
				],
				"typeProperties": {
					"dataset": {
						"referenceName": "DeleteSourceDataset",
						"type": "DatasetReference"
					},
					"logStorageSettings": {
						"linkedServiceName": {
							"referenceName": "AzureBlobStorageSinkLinked",
							"type": "LinkedServiceReference"
						},
						"path": "output"
					},
					"enableLogging": true,
					"storeSettings": {
						"type": "AzureBlobStorageReadSettings",
						"recursive": true,
						"modifiedDatetimeEnd": {
							"value": "utcnow()",
							"type": "Expression"
						},
						"enablePartitionDiscovery": false
					}
				}
			}
		],
		"annotations": []
	}
}